{
    "version": "0.1.dev1",
    "name": "aiida-tb2j",
    "keywords": ["aiida", "tb2j"],
    "description": "A plugin to use the TB2J code within the AiiDA framework.",
    "classifiers": [
	"Programming Language :: Python :: 3.8",
	"Programming Language :: Python :: 3.9",
	"Framework :: AiiDA"
    ],
    "install_requires": [
	"plumpy>=0.15.0",
	"aiida_core>=1.3.0",
	"ase",
        "TB2J"
    ],
    "entry_points": {
        "aiida.calculations": [
            "tb2j.tb2j = aiida_tb2j.calculations.tb2j:TB2JCalculation",
            "tb2j.tb2j_openmx = aiida_tb2j.calculations.tb2j_openmx:TB2JOpenMXCalculation"
        ],
        "aiida.parsers": [
            "tb2j.parser = aiida_tb2j.parsers.tb2j:TB2JParser",
            "tb2j.basic = aiida_tb2j.parsers.basic:BasicParser"
        ],
        "aiida.workflows": [
        ],
        "aiida.data": [
            "tb2j.exchange = aiida_tb2j.data.exchange:ExchangeData"
        ]
    }
}
